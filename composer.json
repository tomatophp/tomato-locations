{
    "name": "tomatophp/tomato-locations",
    "type": "library",
    "description": "Database seeds for Locations plugin for TomatoPHP",
    "keywords": [
        "php",
        "laravel",
        "tomatophp",
        "splade",
        "countires",
        "cities",
        "area",
        "languages",
        "currency"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\TomatoLocations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\TomatoLocations\\TomatoLocationsServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "tomatophp/tomato-roles": "^1.1",
        "tomatophp/tomato-admin": "^1.2",
        "tomatophp/tomato-settings": "^1.1"
    }
}
